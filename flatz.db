CREATE TABLE users (
    id INTEGER PRIMARY KEY,
    name TEXT,
    community TEXT,
    interests TEXT
);
CREATE TABLE items (
    id INTEGER PRIMARY KEY,
    title TEXT,
    description TEXT,
    tags TEXT,
    creator_id INTEGER,
    timestamp TEXT
);
CREATE TABLE interactions (
    id INTEGER PRIMARY KEY,
    user_id INTEGER,
    item_id INTEGER,
    type TEXT,
    timestamp TEXT
);
